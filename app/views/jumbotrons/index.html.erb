<div class="container mt-3 mb-5">
  <div class="row">
    <div class = "col-md-4 col-lg-3">
      <%= render partial: 'shared/admin_sidebar' , locals: {current: 'jumbotron'}%>
    </div>
    <div class="col-md-8 col-lg-9">
      <div class="col-sm-12 d-flex align-items-center justify-content-between">
        <h1> Jumbotron </h1>
      </div>
      <div class="row">
        <% unless @jumbotron.presence%>
          <div class="col-12">
            <p>Yor have no jumbotrons, <%= link_to 'Create a new Jumbotron', new_jumbotron_path %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>